@model IEnumerable<Inventory>

@{
    ViewBag.Title = "Home | Inventory";
}

<p>@Model.Count() record(s)</p>

<table class="table">
    <tr>
        <td><button data-get="/Home/Insert">Insert</button></td>
    </tr>
    <tr>
        <th>Id</th>
        <th>Expiry Date</th>
        <th>Product Id</th>
        <th></th>
    </tr>
    @foreach (var s in Model)
    {
        <tr>
            <td>@s.Id</td>
            <td>@s.ExpiryDate</td>
            <td>@s.ProductId</td>
            <td>
                <button data-get="/Home/Update/@s.Id">Update</button>
                <button data-post="/Home/Delete/@s.Id">Delete</button>
            </td>
        </tr>
    }

</table>