@model Supplier

@{
    ViewBag.Title = "Suppliers | Detail";
}

<iframe id="map-iframe" width="600" height="450" frameborder="0" style="border:0" allowfullscreen></iframe>

<table class="table detail">
    <tr>
        <th>Id</th>
        <td>@Model.Id</td>
    </tr>
    <tr>
        <th>Name</th>
        <td>@Model.Name</td>
    </tr>
    <tr>
        <th>Supplier Type</th>
        <td>@Model.SupplierType</td>
    </tr>
    <tr>
        <th>Address</th>
        <td>@Model.Address</td>
    </tr>
    <tr>
        <th>Contact Number</th>
        <td>@Model.ContactNo</td>
    </tr>
</table>
