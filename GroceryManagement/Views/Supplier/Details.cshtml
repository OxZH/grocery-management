@model Supplier

@{
    ViewBag.Title = "Suppliers | Detail";
    var encodedAddress = Uri.EscapeDataString(Model.Address);
}

<iframe
  class="map-embed"
  src="https://www.google.com/maps?q=@encodedAddress&output=embed"
  loading="lazy">
</iframe>
<br/>

<table class="table detail">
    <tr>
        <th>Id</th>
        <td>@Model.Id</td>
    </tr>
    <tr>
        <th>Name</th>
        <td>@Model.Name</td>
    </tr>
    <tr>
        <th>Supplier Type</th>
        <td>@Model.SupplierType</td>
    </tr>
    <tr>
        <th>Address</th>
        <td>@Model.Address</td>
    </tr>
    <tr>
        <th>Contact Number</th>
        <td>@Model.ContactNo</td>
    </tr>
</table>
